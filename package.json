{
  "name": "find-webpack",
  "version": "0.0.0-development",
  "description": "Utility to find webpack settings in react-scripts and other situations",
  "main": "src",
  "files": [
    "src"
  ],
  "scripts": {
    "test:getWebpackOptions:tryReactScripts": "cd tests/getWebpackOptions/react-scripts && npm test",
    "test:getWebpackOptions:tryEjectedReactScripts": "cd tests/getWebpackOptions/ejected-react-scripts && npm test",
    "test:getWebpackOptions:tryVueCLIScripts": "cd tests/getWebpackOptions/vue-cli-scripts && npm test",
    "test:getWebpackOptions:tryRootProjectWebpack": "cd tests/getWebpackOptions/root-project-webpack && npm test",
    "test:getWebpackOptions:tryWebpackInReactScriptsModule": "cd tests/getWebpackOptions/webpack-in-react-scripts-module/parent/1/2/3/4 && npm test",
    "test": "npm run test:getWebpackOptions:tryReactScripts && npm run test:getWebpackOptions:tryEjectedReactScripts && npm run test:getWebpackOptions:tryVueCLIScripts && npm run test:getWebpackOptions:tryRootProjectWebpack && npm run test:getWebpackOptions:tryWebpackInReactScriptsModule",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/find-webpack.git"
  },
  "keywords": [
    "webpack",
    "utility"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/find-webpack/issues"
  },
  "homepage": "https://github.com/bahmutov/find-webpack#readme",
  "dependencies": {
    "debug": "4.1.1",
    "find-yarn-workspace-root": "1.2.1",
    "mocked-env": "1.3.2"
  },
  "devDependencies": {
    "prettier": "2.0.5",
    "semantic-release": "^17.0.4",
    "uvu": "^0.5.6"
  }
}
